	<script>
		var token = '@Antiforgery.GetAndStoreTokens(Context).RequestToken';

			window.addEventListener('popstate', function () {
				fetch('/Home/ClearSession', {
					method: 'POST',
					headers: {
						'RequestVerificationToken': token
					}
				}).then(() => {
					// Redirect to login page after session cleared
					window.location.href = '/Home/Login';
				});
			});

		$(document).ready(function () {
			// Get the antiforgery token from server
			


			let idleTime = 0;
			const logoutAfter = 120; // seconds (2 minutes)

			// increment idle time every second
			setInterval(() => {
				idleTime++;
				if (idleTime >= logoutAfter) {
					window.location.href = '/Home/Logout';
				}
			}, 1000);

			// reset idle time on activity
			$(document).on('mousemove keypress click scroll', function () {
				idleTime = 0;
			});
		});

		
	

	</script>



