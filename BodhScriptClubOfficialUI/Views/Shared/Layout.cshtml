			top: 0;
			left: 0;
			right: 0;
			height: 72px; /* fixed pixel height */
			display: flex;
			align-items: center;
			gap: 1rem;
			padding: 0 1.25rem;
			z-index: 50;
			color: white;
			backdrop-filter: blur(6px);
			-webkit-backdrop-filter: blur(6px);
			border-bottom: 1px solid rgba(255,255,255,0.06);
			box-shadow: 0 6px 18px rgba(8,10,20,0.25);
			overflow: hidden;
		}


	/* Animated gradient background (uses background-position animation) */
	#changeheadercolor {
	--grad-size: 240% 240%;
	background: linear-gradient(90deg,
	#ff6b6b 0%,
	#ffb86b 20%,
	#ffd36b 35%,
	#9be15d 50%,
	#6bd1ff 70%,
	#8a6bff 85%,
	#ff6bb8 100%);
	background-size: var(--grad-size);
	transition: box-shadow 300ms ease, transform 300ms ease;
	animation: gradientShift 12s linear infinite;
	}

	/* soft glow on top */
	#changeheadercolor::before{
	content: "";
	position: absolute;
	inset: 0;
	pointer-events: none;
	mix-blend-mode: screen;
	opacity: 0.18;
	background: radial-gradient(600px 200px at 10% 20%, rgba(255,255,255,0.25), transparent 12%),
	radial-gradient(400px 160px at 90% 40%, rgba(255,255,255,0.14), transparent 10%);
	transition: opacity 300ms ease;
	}

	/* subtle floating motion */
	@@keyframes floatHeader {
	0%   { transform: translateY(0px); }
	50%  { transform: translateY(-3px); }
	100% { transform: translateY(0px); }
	}

	/* the main gradient animation (shift background position) */
	@@keyframes gradientShift {
	0%   { background-position: 0% 50%; }
	50%  { background-position: 100% 50%; }
	100% { background-position: 0% 50%; }
	}

	/* hover/active styles */
	#changeheadercolor:hover {
	box-shadow: 0 10px 30px rgba(12,14,30,0.45);
	transform: translateY(-2px);
	animation-duration: 8s; /* speed up on hover */
	}

	/* class for "paused" or slow state (e.g., when user scrolls) */
	#changeheadercolor.slow {
	animation-play-state: running;
	animation-duration: 30s; /* slow */
	--glow-opacity: 0.08;
	}
	#changeheadercolor.paused {
	animation-play-state: paused;
	transform: none;
	}

	/* small logo / title */
	.brand { font-weight: 700; letter-spacing: .3px; font-size: 1.05rem; display:flex; align-items:center; gap:.5rem; z-index:3; }
	.brand .dot {
	width:12px; height:12px; border-radius:50%;
	background: rgba(255,255,255,0.95);
	filter: drop-shadow(0 2px 6px rgba(0,0,0,0.25));
	}

	/* nav/action area */
	nav { margin-left:auto; display:flex; align-items:center; gap:.5rem; z-index:3; }
	.btn {
	background: rgba(255,255,255,0.12);
	color: white;
	border: 1px solid rgba(255,255,255,0.12);
	padding: .45rem .6rem;
	border-radius: .5rem;
	font-size: .9rem;
	cursor: pointer;
	transition: transform 180ms ease, background 180ms ease;
	}
	.btn:active { transform: scale(.98); }

	/* prefer-reduced-motion: stop the animation for users who prefer minimal motion */
	@@media (prefers-reduced-motion: reduce) {
	#changeheadercolor { animation: none !important; }
	}

	/* small responsive tweaks */
	@@media (max-width:640px) {
	header.navbar { padding: 0 .8rem; height:64px; }
	.brand { font-size: .95rem; }
	.btn { font-size: .85rem; padding: .35rem .5rem; }
	}

	/* content below header so you can scroll and observe effects */
	main { padding-top: 110px; max-width:980px; margin: 0 auto; }
	.hero { height: 64vh; display:grid; place-items:center; text-align:center; color:#111; }
	.card { background: white; border-radius: 12px; padding: 1.25rem; box-shadow: 0 6px 22px rgba(10,12,20,0.06); }
		/* SIDE PANEL */
		.side-panel {
			position: fixed;
			top: 0;
			right: -360px;
			width: 320px;
			height: 100vh;
			background: #fff;
			box-shadow: -10px 0 30px rgba(0,0,0,0.2);
			transition: right 0.5s cubic-bezier(0.77,0,0.175,1);
			z-index: 1050;
			display: flex;
			flex-direction: column;
			border-left: 1px solid #eee;
			border-radius: 8px 0 0 8px;
		}

			/* Open Panel */
			.side-panel.open {
				right: 0;
			}

			/* PANEL HEADER WITH GRADIENT */
			.side-panel .pro-head {
				background: linear-gradient(135deg, #6a11cb 0%, #2575fc 100%);
				color: #fff;
				display: flex;
				justify-content: space-between;
				align-items: center;
				padding: 20px;
				border-radius: 8px 0 0 0;
			}

		/* USER INFO */
		.user-info {
			display: flex;
			align-items: center;
			gap: 10px;
		}

		.avatar-ring {
			position: relative;
			width: 50px;
			height: 50px;
			border-radius: 50%;
			padding: 3px;
			background: linear-gradient(45deg, #ff6a00, #ee0979);
		}

			.avatar-ring img {
				width: 100%;
				height: 100%;
				border-radius: 50%;
				display: block;
			}

		/* CLOSE BUTTON */
		.close-btn {
			background: none;
			border: none;
			color: #fff;
			font-size: 1.2rem;
			cursor: pointer;
			transition: transform 0.2s;
		}

			.close-btn:hover {
				transform: rotate(90deg);
			}

		/* MENU ITEMS */
		.pro-body {
			list-style: none;
			padding: 0;
			margin: 0;
			flex: 1;
			overflow-y: auto;
		}

			.pro-body li {
				border-bottom: 1px solid #f0f0f0;
			}

			.pro-body a {
				display: flex;
				align-items: center;
				padding: 14px 20px;
				color: #333;
				font-weight: 500;
				text-decoration: none;
				transition: all 0.3s;
				border-radius: 8px;
			}

				.pro-body a i {
					margin-right: 12px;
					color: #2575fc;
					transition: transform 0.3s;
				}

				.pro-body a:hover {
					background: #2575fc;
					color: #fff;
				}

					.pro-body a:hover i {
						transform: scale(1.2) rotate(10deg);
					}

		/* OVERLAY */
		.overlay {
			position: fixed;
			inset: 0;
			background: rgba(0,0,0,0.45);
			opacity: 0;
			pointer-events: none;
			transition: opacity 0.3s ease;
			z-index: 1040;
		}

			.overlay.show {
				opacity: 1;
				pointer-events: auto;
			}


	</style>
</head>

